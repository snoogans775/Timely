<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController.EventController">
   <children>
      <VBox alignment="CENTER" prefHeight="100.0" prefWidth="400.0">
         <children>
            <Label text="Events" />
         </children>
      </VBox>
      <VBox layoutY="100.0" prefHeight="400.0" prefWidth="400.0">
         <children>
            <TableView prefHeight="300.0" prefWidth="400.0">
              <columns>
                <TableColumn prefWidth="100.0" text="Title" />
                <TableColumn prefWidth="100.0" text="Customer" />
                  <TableColumn prefWidth="100.0" text="Start" />
                  <TableColumn prefWidth="100.0" text="End" />
              </columns>
            </TableView>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Button mnemonicParsing="false" text="Add">
                  </Button>
                  <Button mnemonicParsing="false" text="Edit" />
                  <Button mnemonicParsing="false" text="Delete" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox layoutY="500.0" prefHeight="100.0" prefWidth="400.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="200.0" text="Events">
                  </Button>
                  <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="200.0" text="Customers" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
